package flash.text.engine {


/**
 * The FontWeight class is an enumeration of constant values used with <code>FontDescription.fontWeight</code>.
 * @see FontDescription#fontWeight
 *
 */
public final class FontWeight {
  /**
   * Used to indicate bold font weight.
   */
  public static const BOLD:String = "bold";
  /**
   * Used to indicate normal font weight.
   */
  public static const NORMAL:String = "normal";
}
}
