package flashx.textLayout.formats {


/**
 * Defines a constant for specifying that the value of the <code>backgroundColor</code> property of the <code>TextLayoutFormat</code> class is "transparent".
 * @see TextLayoutFormat#backgroundColor
 *
 */
public final class BackgroundColor {
  /**
   * Transparent - no background color is applied.
   */
  public static const TRANSPARENT:String = "transparent";
}
}
